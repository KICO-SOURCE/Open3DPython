{
  "name": "@jupyterlab/terminal-extension",
  "version": "3.0.12",
  "description": "JupyterLab - Terminal Emulator Extension",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "sideEffects": [
    "style/**/*.css",
    "style/index.js"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.d.ts",
    "lib/*.js.map",
    "lib/*.js",
    "schema/*.json",
    "style/**/*.css",
    "style/index.js"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "prepublishOnly": "npm run build",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/application": "^3.0.11",
    "@jupyterlab/apputils": "^3.0.9",
    "@jupyterlab/launcher": "^3.0.9",
    "@jupyterlab/mainmenu": "^3.0.9",
    "@jupyterlab/running": "^3.0.9",
    "@jupyterlab/services": "^6.0.9",
    "@jupyterlab/settingregistry": "^3.0.6",
    "@jupyterlab/terminal": "^3.0.10",
    "@jupyterlab/translation": "^3.0.9",
    "@jupyterlab/ui-components": "^3.0.7",
    "@lumino/algorithm": "^1.3.3",
    "@lumino/widgets": "^1.16.1"
  },
  "devDependencies": {
    "@types/webpack-env": "^1.14.1",
    "rimraf": "~3.0.0",
    "typedoc": "~0.20.0-beta.27",
    "typescript": "~4.1.3"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "schemaDir": "schema"
  },
  "styleModule": "style/index.js",
  "gitHead": "6e4b14879f5d6995a6134aa493676694e37c589e"
}
